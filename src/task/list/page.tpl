<!-- 存放内容的模块 -->
<div class="list-wrapper-inner" id="list-wrapper-{{ name }}" data-contentname="{{ name }}">
    <div class="scroll-outter">
        <div class="scroll-inner">
            <!-- 用于重新加载数据 -->
            <div class="data-reload"></div>
            <!-- 数据存放容器 -->
            <div class="list-wrapper-content"></div>
            <!-- 用于加载更多数据 -->
            <div class="data-more"></div>
        </div>
    </div>
</div>